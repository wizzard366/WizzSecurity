= form_for @residential do |f|
  -if @residential.errors.any?
    #error_explanation
      %h2= "#{pluralize(@residential.errors.count, "error")} prohibited this residential from being saved:"
      %ul
        - @residential.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :address
    = f.text_field :address
  .field
    = f.label :type
    = f.number_field :type
  .field
    = f.label :description
    = f.text_area :description
  .field
    = f.label :enabled
    = f.text_field :enabled
  .field
    = f.label :password
    = f.text_field :password
  .actions
    = f.submit 'Save'
